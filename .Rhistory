cn2 <- data.frame(T, pct1cn2, pct5cn2, pct10cn2)
colnames(cn2) <- c("T", "1pct", "5pct", "10pct")
# Test de racine unitaire - Modèle 3 (n)
pct1cn3 <- c(-2.62, -2.60, -2.58, -2.58, -2.58)
pct5cn3 <- c(rep(-1.95, 5))
pct10cn3 <- c(-1.61, -1.61, -1.62, -1.62, -1.62)
cn3 <- data.frame(T, pct1cn3, pct5cn3, pct10cn3)
colnames(cn3) <- c("T", "1pct", "5pct", "10pct")
# Fonction pour déterminer rowselec basée sur N
get_rowselec <- function(N) {
if (is.null(N)) return(NULL)
if (N < 25) return(1)
if (25 <= N & N < 50) return(1)
if (50 <= N & N < 100) return(2)
if (100 <= N & N < 250) return(3)
if (250 <= N & N < 500) return(4)
if (N >= 500) return(5)
}
rowselec <- get_rowselec(N)
# Si model n'est pas spécifié, utiliser les valeurs critiques du test
if (is.null(model) && is.null(N)) {
if (type == "ct") {
return(ct)  # Test tendance
} else if (type == "c") {
return(c)  # Test constante
} else if (type == "n") {
return(cn3)  # Test sans constante ni tendance
}
} else if (!is.null(model) && is.null(N)) {
# Si model est spécifié, retourner les tableaux correspondants
if (model == 1 && type == "ct") {
return(cn1)    # Modèle 1 - constante et tendance
} else if (model == 2 && type == "c") {
return(cn2)  # Modèle 2 - constante seulement
} else if (model == 3 && type == "n") {
return(cn3)  # Modèle 3 - sans constante ni tendance
} else {
stop("Combinaison invalide de model et type")
}
} else if (is.null(model) && !is.null(N)) {
# Si N est spécifié mais pas model
if (type == "ct") {
return(ct[rowselec, ])  # Test tendance
} else if (type == "c") {
return(c[rowselec, ])  # Test constante
} else if (type == "n") {
return(cn3[rowselec, ])  # Test sans constante ni tendance
}
} else if (!is.null(model) && !is.null(N)) {
# Si model et N sont tous les deux spécifiés
if (model == 1 && type == "ct") {
return(cn1[rowselec, ])    # Modèle 1 - constante et tendance
} else if (model == 2 && type == "c") {
return(cn2[rowselec, ])  # Modèle 2 - constante seulement
} else if (model == 3 && type == "n") {
return(cn3[rowselec, ])  # Modèle 3 - sans constante ni tendance
} else {
stop("Combinaison invalide de model et type")
}
}
# Si aucune condition n'est remplie
stop("Paramètres invalides")
}
